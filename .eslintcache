bind(this)   // 新增列表\r\n    }\r\n    // 新增列表\r\n    handlAdd() {\r\n        // prevState 改变之前的数据相当与this.state\r\n        this.setState((prevState) => ({\r\n            list: [...prevState.list, prevState.content],\r\n            content: ''\r\n        }))\r\n    }\r\n    // 搜索事件\r\n    handlChange(e) {\r\n        // 外部对值进行保存\r\n        const value = e.target.value\r\n        // 异步\r\n        this.setState(() => ({ content: value }))\r\n\r\n    }\r\n    // 删除\r\n    handlDel(index) {\r\n        this.setState((prevState) => {\r\n            let list = [...prevState.list]\r\n            list.splice(index, 1)\r\n            return { list }\r\n        })\r\n    }\r\n    // 数据请求\r\n    componentDidMount(){\r\n        // axios.get('').then(()=>{\r\n        //     alert('succ')\r\n        // })\r\n        // .catch(()=>{\r\n        //     alert('error')\r\n        // })\r\n    }\r\n    render() {\r\n        return (\r\n            <Fragment>\r\n                <Input\r\n                    placeholder=\"input search text\"\r\n                    allowClear\r\n                    style={{ width: 300, margin: '20px' }}\r\n                    value={this.state.content}\r\n                    onChange={this.handlChange}\r\n                />\r\n                <Button type=\"primary\" onClick={this.handlAdd}>Add</Button>\r\n                <TodoItem list={this.state.list} deleteItem={this.handlDel} />\r\n\r\n            </Fragment>\r\n        );\r\n    }\r\n}\r\n\r\nexport default todoList;","D:\\ReactProjects\\React-dom\\src\\todoList\\components\\todoItem.js",["24"],{"ruleId":"25","severity":1,"message":"26","line":10,"column":25,"nodeType":"27","messageId":"28","endLine":10,"endColumn":29},{"ruleId":"25","severity":1,"message":"29","line":10,"column":31,"nodeType":"27","messageId":"28","endLine":10,"endColumn":41},{"ruleId":"25","severity":1,"message":"30","line":10,"column":43,"nodeType":"27","messageId":"28","endLine":10,"endColumn":50},{"ruleId":"25","severity":1,"message